<%= form_for [:admin, @router] do |f| %>
<%= f.text_field :name, placeholder: "name" %>
<%= f.text_field :address, placeholder: "address" %>
<%= f.text_field :description, placeholder: "des" %>
<%= f.text_field :latitude, placeholder: "latitude" %>
<%= f.text_field :longitude, placeholder: "longtitude" %>
<%= f.submit 'creater' %>
<% end %>